<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vxappstore"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="14" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.vxmobilecomm.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/store"
        android:label="@string/app_name"
        android:theme="@style/AppBaseTheme_actionbar" >
        <activity
            android:name="com.vxmobilecomm.activity.MovingBackground"
            android:label="@string/app_name"
            android:theme="@style/AppBaseTheme_noactionbar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.vxmobilecomm.activity.VXMobileComm"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppBaseTheme_noactionbar" >
        </activity>
        <activity
            android:name="com.vxmobilecomm.activity.ProductView"
            android:label="@string/app_name"
            android:screenOrientation="landscape"
            android:theme="@style/AppBaseTheme_noactionbar" >
        </activity>
        <activity
            android:name="com.vxmobilecomm.activity.Search"
            android:label="@string/app_name"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"
            android:theme="@style/AppBaseTheme_noactionbar" >
        </activity>
    </application>

</manifest>